{
  "version": "3.4.0-*",
  "description": "NuGet 3 restore for dotnet CLI, DNX, and UWP",
  "compilationOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true
  },
  "dependencies": {
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc1-final",
    "Microsoft.NETCore.Platforms": "1.0.1-beta-*",
    "NuGet.Commands": {
      "target": "project"
    },
    "NuGet.Shared": {
      "type": "build",
      "target": "project"
    },
    "System.Runtime.Serialization.Primitives": "4.0.0",
    "Microsoft.Extensions.CommandLineUtils.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    }
  },
  "commands": {
    "NuGet.CommandLine.XPlat": "NuGet.CommandLine.XPlat"
  },
  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "14.0.0",
        "Microsoft.Build": "14.0.0",
        "System.Collections": ""
      }
    },
    "netstandard1.5": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.0.0-rc2-23811",
        "System.Console": "4.0.0-rc2-23811"
      }
    }
  }
}